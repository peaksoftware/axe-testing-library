{
  "name": "axe-testing-library",
  "version": "0.1.0",
  "description": "Extensions for automated aXe accessibility testing for popular test tooling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "npm run jest && npm run vitest && npm run playwright",
    "jest": "jest",
    "playwright": "npx playwright test",
    "vitest": "vitest run",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "testing",
    "jest",
    "playwright",
    "puppeteer",
    "mocha",
    "jasmine",
    "axe",
    "test-runner"
  ],
  "author": "alex@peaksoftware.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ts-a11y-tester.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axe-core": "^4.10.3",
    "playwright-core": "^1.50.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "rollup": "^4.34.9",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
